<ROSETTASCRIPTS>
   <SCOREFXNS>
       <ScoreFunction name="stage1" weights="score3" symmetric="0">
           <Reweight scoretype="atom_pair_constraint" weight="0.5"/>
       </ScoreFunction>
       <ScoreFunction name="stage2" weights="score4_smooth_cart" symmetric="0">
           <Reweight scoretype="atom_pair_constraint" weight="0.5"/>
       </ScoreFunction>
       <ScoreFunction name="fullatom" weights="talaris2013_cart" symmetric="0">
           <Reweight scoretype="atom_pair_constraint" weight="0.5"/>
       </ScoreFunction>
   </SCOREFXNS>
   <RESIDUE_SELECTORS>
       <Chain name="chA" chains="A"/>
   </RESIDUE_SELECTORS>
   <SIMPLE_METRICS>
       <RMSDMetric name="rmsd" rmsd_type="rmsd_protein_bb_heavy" use_native="1"/>
   </SIMPLE_METRICS>
   <MOVERS>
       <Superimpose name="superimpose" CA_only="0"/>
       <RunSimpleMetrics name="run_metrics" metrics="rmsd"/>
       <Hybridize name="hybridize" stage1_scorefxn="stage1" stage2_scorefxn="stage2" fa_scorefxn="fullatom" batch="1" stage1_increase_cycles="1.0" stage2_increase_cycles="1.0">
           <Template pdb="%ROSETTA%/tests/scientific/data/%TEST%/1EM2_A_4E6F_A.pdb" cst_file="AUTO" weight=1.000 />
           <Template pdb="%ROSETTA%/tests/scientific/data/%TEST%/1IFV_B_4E6F_A.pdb" cst_file="AUTO" weight=1.000 />
           <Template pdb="%ROSETTA%/tests/scientific/data/%TEST%/1TW0_B_4E6F_A.pdb" cst_file="AUTO" weight=1.000 />
           <Template pdb="%ROSETTA%/tests/scientific/data/%TEST%/1XUV_B_4E6F_A.pdb" cst_file="AUTO" weight=1.000 />
           <Template pdb="%ROSETTA%/tests/scientific/data/%TEST%/2D4R_D_4E6F_A.pdb" cst_file="AUTO" weight=1.000 />
           <Template pdb="%ROSETTA%/tests/scientific/data/%TEST%/2KF2_A_4E6F_A.pdb" cst_file="AUTO" weight=1.000 />
           <Template pdb="%ROSETTA%/tests/scientific/data/%TEST%/2LPX_A_4E6F_A.pdb" cst_file="AUTO" weight=1.000 />
           <Template pdb="%ROSETTA%/tests/scientific/data/%TEST%/3OTL_A_4E6F_A.pdb" cst_file="AUTO" weight=1.000 />
           <Template pdb="%ROSETTA%/tests/scientific/data/%TEST%/3PUT_B_4E6F_A.pdb" cst_file="AUTO" weight=1.000 />
           <Template pdb="%ROSETTA%/tests/scientific/data/%TEST%/3Q64_A_4E6F_A.pdb" cst_file="AUTO" weight=1.000 />
       </Hybridize>
       <FastRelax name="relax" scorefxn="fullatom"/>
   </MOVERS>
   <PROTOCOLS>
       <Add mover=hybridize/>
       <Add mover="relax"/>
       <Add mover="superimpose"/>
       <Add mover="run_metrics"/>
   </PROTOCOLS>
</ROSETTASCRIPTS>